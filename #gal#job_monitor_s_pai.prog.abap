FORM pai_0101_user_command .
  CASE sy-ucomm.
    WHEN 'SEARCH'.
      PERFORM pbo_0101_load_data_into_grid.
  ENDCASE.
ENDFORM.                    " PAI_0101_USER_COMMAND

